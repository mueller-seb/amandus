subsection Discretization
  set FE         = FE_DGQ(2)
  set Refinement = 1
end
subsection ThetaTimestepping
  subsection ThetaTimestepping
    set Adaptive   = false
    set Final      = 1.
    set First step = .1
    set Max step   = 1.
    set Print step = -1.
    set Start      = 0.
    set Strategy   = uniform
    set Theta      = .5
    set Tolerance  = 1.e-2
  end
end
